<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.with_tnt.myapplication3">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_TASKS"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:theme="@style/AppTheme"
        android:label="@string/app_name"
        android:supportsRtl="true">


        <activity android:name=".login_signup.LoginActivity"
            android:theme="@style/loginTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->

        <activity android:name=".login_signup.JoinActivity"  android:theme="@style/loginTheme"></activity>

        <activity android:name=".main.MainActivity"></activity>

        <activity android:name=".notice.NoticeActivity"
            android:theme="@style/OnlyTitleTheme"> </activity>

        <activity android:name=".notice.NoticeDetailActivity"
            android:theme="@style/OnlyTitleTheme"> </activity>

        <activity android:name=".product.ProductlistActivity"
            android:theme="@style/OnlyTitleTheme"></activity>

        <activity android:name=".product.ViewProductActivity"
            android:theme="@style/OnlyTitleTheme" ></activity>

        <activity android:name=".product.UploadProductActivity"
            android:theme="@style/OnlyTitleTheme"></activity>

        <activity android:name=".product.ProductCategoryActivity"></activity>
        <activity android:name=".product.ModifyProductActivity"></activity>
        <activity android:name=".cart.CartActivity"/>
        <activity android:name=".chat.ChattingActivity"/>

        <activity android:name=".login_signup.find_info.Find_Info_Activity" />
        <activity
            android:name=".Setting.SettingsActivity"
            android:label="@string/title_activity_settings" />


        <service android:name="com.with_tnt.psuh.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name="com.with_tnt.psuh.MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>


        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>